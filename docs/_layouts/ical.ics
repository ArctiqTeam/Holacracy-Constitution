layout: none
output_ext: .ics
---
BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//ArctiqTeam//EN
BEGIN:VTIMEZONE
TZID:{{ page.timezone }}{% if page.timezone == "America/Toronto" %}
X-LIC-LOCATION:America/Toronto
BEGIN:DAYLIGHT
TZOFFSETFROM:-0500
TZOFFSETTO:-0400
TZNAME:EDT
DTSTART:19700308T020000
RRULE:FREQ=YEARLY;BYMONTH=3;BYDAY=2SU
END:DAYLIGHT
BEGIN:STANDARD
TZOFFSETFROM:-0400
TZOFFSETTO:-0500
TZNAME:EST
DTSTART:19701101T020000
RRULE:FREQ=YEARLY;BYMONTH=11;BYDAY=1SU
END:STANDARD
END:VTIMEZONE{% endif %}{% if page.timezone == "America/Vancouver" or page.timezone == "America/Los_Angeles" %}
X-LIC-LOCATION:{{ page.timezone }}
BEGIN:DAYLIGHT
TZOFFSETFROM:-0800
TZOFFSETTO:-0700
TZNAME:PDT
DTSTART:19700308T020000
RRULE:FREQ=YEARLY;BYMONTH=3;BYDAY=2SU
END:DAYLIGHT
BEGIN:STANDARD
TZOFFSETFROM:-0700
TZOFFSETTO:-0800
TZNAME:PST
DTSTART:19701101T020000
RRULE:FREQ=YEARLY;BYMONTH=11;BYDAY=1SU
END:STANDARD
END:VTIMEZONE{% endif %}
BEGIN:VEVENT
UID:{{ page.date | date: "%Y%m%d" }}@arctiq.ca
ORGANIZER;CN="Holly Case":MAILTO:sales@arctiq.ca
DTSTAMP;TZID={{ page.timezone }}:{{ page.date | date: "%Y%m%d" }}{{ page.start | date: "T%H%M" }}00
DTSTART;TZID={{ page.timezone }}:{{ page.date | date: "%Y%m%d" }}{{ page.start | date: "T%H%M" }}00
DTEND;TZID={{ page.timezone }}:{{ page.date | date: "%Y%m%d" }}{{ page.end | date: "T%H%M" }}00
SUMMARY:'{{ page.title | replace: "</br>", "" }}'
LOCATION:'{{ page.location }}'
END:VEVENT
END:VCALENDAR
